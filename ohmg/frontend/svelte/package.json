{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "dev:Index": "rollup -c -w --configComponent=Index",
    "dev:Map": "rollup -c -w --configComponent=Map",
    "dev:Resource": "rollup -c -w --configComponent=Resource",
    "dev:Viewer": "rollup -c -w --configComponent=Viewer",
    "dev:Split": "rollup -c -w --configComponent=Split",
    "dev:Georeference": "rollup -c -w --configComponent=Georeference",
    "dev:SessionList": "rollup -c -w --configComponent=SessionList",
    "start": "sirv public"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.9",
    "@eslint/js": "^9.24.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^16.0.0",
    "phosphor-svelte": "^1.3.0",
    "prettier": "^3.5.3",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "svelte": "^3.59.2"
  },
  "dependencies": {
    "@petamoriken/float16": "3.4.7",
